{
  "expo": {
    "name": "Art4House",
    "slug": "art4house",
    "scheme": "art4house",
    "version": "1.1.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-media-library",
        {
          "granularPermissions": ["photo"]
        }
      ]
    ],
    "android": {
      "package": "com.art4house.tryon",
      "versionCode": 9,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000000"
      }
    },
    "extra": {
      "eas": {
        "projectId": "968ef27a-482d-4c88-8003-2a678245ed5a"
      }
    }
  }
}
